<br>
<h1>Add a Pairing:</h1>
<br>
<%= form_for(@pairing) do |f| %>
<%=f.collection_select :wine_id, Wine.all, :id, :wine_names %>
<%=f.collection_select :cheese_id, Cheese.all, :id, :cheese_names %>
<%= f.submit 'Create Pairing' %>
<% end %>

<br><br><br>
<%= link_to "Add A New Wine", new_wine_path %>
<br><br>
<%= link_to "Add A New Cheese", new_cheese_path %>
